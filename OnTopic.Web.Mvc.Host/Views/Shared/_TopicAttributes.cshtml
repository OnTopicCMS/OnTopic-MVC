@using OnTopic.ViewModels
@model TopicViewModel

<h2>Topic</h2>
<ul>
  <li>Id: @Model.Id</li>
  <li>Key: @Model.Key</li>
  <li>UniqueKey: @Model.UniqueKey</li>
  <li>WebPath: @Model.WebPath</li>
  <li>IsHidden? @Model.IsHidden</li>
  <li>LastModified: @Model.LastModified</li>
  <li>View: @Model.View</li>
</ul>

